{
  "name": "speechstate",
  "version": "0.2.0",
  "license": "GPL-3.0",
  "homepage": "http://localhost/speechstate",
  "private": true,
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1"
  },
  "scripts": {
    "start": "parcel src/index.tsx",
    "build": "parcel build src/index.tsx --no-source-maps --dist-dir dist/",
    "serve": "npx serve dist/ -l 80",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "rm -rf dist && parcel build src/index.tsx --no-source-maps --dist-dir dist/",
    "deploy": "gh-pages -d dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**/*.html"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.0",
    "@svgdotjs/svg.js": "^3.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/webspeechapi": "^0.0.29",
    "@xstate/inspect": "^0.7.0",
    "@xstate/react": "^3.0.1",
    "buffer": "^5.5.0",
    "gh-pages": "^3.2.3",
    "microsoft-cognitiveservices-speech-sdk": "^1.24.1",
    "node-sass": "^6.0.1",
    "parcel": "^2.8.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "punycode": "^1.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "serve": "^12.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.0",
    "typescript": "^4.4.2",
    "util": "^0.12.3",
    "web-speech-cognitive-services": "^7.1.3",
    "web-vitals": "^2.1.0",
    "xstate": "^4.34.0"
  }
}
